<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="net.samuelcmace.utk.gui.controller.KanjiEditorController" prefHeight="600.0" prefWidth="800.0">

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <TextArea fx:id="noteEditor"/>

    <HBox alignment="CENTER" spacing="20.0">
        <Button text="Reset" onAction="#onClick_resetButton"/>
        <Button text="Save" onAction="#onClick_saveButton"/>
        <Button text="Return" onAction="#onClick_returnButton"/>
    </HBox>

</VBox>
